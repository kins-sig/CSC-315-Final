<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <gradient android:angle="-90" android:startColor="@color/uncw_yellow_button_pressed_start" android:endColor="@color/uncw_yellow_button_pressed_end"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <gradient android:angle="-90" android:startColor="@color/uncw_yellow_button_start" android:endColor="@color/uncw_yellow_button_end" />
        </shape>
    </item>
</selector>